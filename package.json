{
  "name": "woreact",
  "version": "0.0.1",
  "private": true,
  "config": {
    "worker": "--config config/webpack/worker.config.js",
    "client": "--config config/webpack/client.config.js"
  },
  "main": "index.js",
  "scripts": {
    "dev": "wrangler dev --local",
    "deploy": "NODE_ENV=production wrangler publish",
    "client": "webpack $npm_package_config_client",
    "worker": "BABEL_ENV=worker webpack $npm_package_config_worker",
    "build": "webpack $npm_package_config_client && BABEL_ENV=worker webpack $npm_package_config_worker",
    "production": "NODE_ENV=production webpack $npm_package_config_client && NODE_ENV=production BABEL_ENV=worker webpack $npm_package_config_worker",
    "typecheck": "tsc --noEmit"
  },
  "browserslist": [
    "last 2 years",
    "> 5%",
    "not IE 11"
  ],
  "pre-commit": [
    "typecheck"
  ],
  "author": "ogarich89 <ogarich89@icloud.com>",
  "license": "MIT",
  "dependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0",
    "@babel/plugin-transform-modules-commonjs": "^7.10.4",
    "@babel/plugin-transform-runtime": "^7.6.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.6.0",
    "@babel/register": "^7.17.0",
    "@babel/runtime-corejs3": "^7.6.0",
    "@cloudflare/kv-asset-handler": "^0.2.0",
    "@types/classnames": "^2.2.9",
    "@types/fbemitter": "^2.0.32",
    "@types/node": "^17.0.15",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-redux": "^7.1.4",
    "@types/react-router-dom": "^5.1.0",
    "@types/redux": "^3.6.0",
    "@types/redux-devtools-extension": "^2.13.2",
    "@types/redux-thunk": "^2.1.0",
    "@typescript-eslint/eslint-plugin": "^5.10.2",
    "@typescript-eslint/parser": "^5.10.2",
    "autoprefixer": "^10.0.1",
    "babel-loader": "^8.0.0",
    "classnames": "^2.2.6",
    "core-js": "^3.2.1",
    "css-loader": "^6.6.0",
    "cssnano": "^5.0.2",
    "eslint": "^8.8.0",
    "eslint-plugin-react": "^7.11.1",
