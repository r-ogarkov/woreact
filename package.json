{
  "name": "woreact",
  "version": "0.0.1",
  "private": true,
  "config": {
    "worker": "--config config/webpack/worker.config.js",
    "client": "--config config/webpack/client.config.js"
  },
  "main": "index.js",
  "scripts": {
    "dev": "wrangler dev --local",
    "deploy": "NODE_ENV=production wrangler publish",
    "client": "webpack -w $npm_package_config_client",
    "worker": "SWC_ENV=worker webpack -w $npm_package_config_worker",
    "build": "webpack $npm_package_config_client && SWC_ENV=worker webpack $npm_package_config_worker",
    "production": "NODE_ENV=production webpack $npm_package_config_client && NODE_ENV=production SWC_ENV=worker webpack $npm_package_config_worker",
    "typecheck": "tsc --noEmit"
  },
  "browserslist": [
    "last 2 years",
    "> 5%",
    "not IE 11"
  ],
  "pre-commit": [
    "typecheck"
  ],
  "author": "ogarich89 <ogarich89@icloud.com>",
  "license": "MIT",
  "dependencies": {
    "@cloudflare/kv-asset-handler": "^0.2.0",
    "@swc/core": "^1.2.197",
    "@types/classnames": "^2.2.9",
    "@types/fbemitter": "^2.0.32",
    "@types/node": "^17.0.15",
    "@types/react": "^18.0.12",
    "@types/react-dom": "^18.0.5",
    "@types/react-redux": "^7.1.4",
    "@types/react-router-dom": "^5.1.0",
    "@types/redux": "^3.6.0",
    "@types/redux-devtools-extension": "^2.13.2",
    "@types/redux-thunk": "^2.1.0",
    "@typescript-eslint/eslint-plugin": "^5.10.2",
    "@typescript-eslint/parser": "^5.10.2",
    "autoprefixer": "^10.0.1",
    "classnames": "^2.2.6",
    "css-loader": "^6.7.1",
    "cssnano": "^5.1.11",
    "eslint": "^8.8.0",
    "eslint-plugin-react": "^7.11.1",
    "eslint-webpack-plugin": "^3.1.1",
    "fbemitter": "^3.0.0",
    "handlebars": "^4.7.7",
    "handlebars-loader": "^1.7.1",
    "i18next": "^21.8.9",
    "i18next-fetch-backend": "^5.0.0",
    "identity-obj-proxy": "^3.0.0",
    "imagemin-webpack-plugin": "^2.2.0",
    "mini-css-extract-plugin": "^2.6.0",
    "postcss": "^8.4.14",
    "postcss-combine-media-query": "^1.0.1",
    "postcss-import": "^14.1.0",
    "postcss-loader": "^7.0.0",
    "postcss-scss": "^4.0.4",
    "pre-commit": "^1.2.2",
    "query-string": "^7.1.1",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-i18next": "^11.17.0",
    "react-redux": "^7.0.3",
    "react-router-dom": "^6.2.1",
    "redux": "^4.0.1",
    "redux-devtools-extension": "^2.13.5",
    "redux-thunk": "^2.3.0",
    "reset-css": "^5.0.1",
    "sass": "^1.52.2",
    "sass-loader": "^13.0.0",
    "stylelint": "^14.8.5",
    "stylelint-webpack-plugin": "^3.3.0",
    "svg-inline-loader": "^0.8.2",
    "swc-loader": "^0.2.1",
    "typescript": "^4.0.3",
    "url-loader": "^4.1.1",
    "webpack": "^5.1.3",
    "webpack-cli": "^4.1.0",
